{
    "tasks": {
        "dev": "deno run -A --node-modules-dir=auto npm:vite",
        "build": "deno run -A --node-modules-dir=auto npm:vite build",
        "server:start": "deno run -A --node-modules-dir --watch ./server/main.ts",
        "serve": "deno task build && deno task server:start"
    },
    "imports": {
        "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.4",
        "@oak/oak": "jsr:@oak/oak@^17.1.4",
        "@std/assert": "jsr:@std/assert@^1.0.12",
        "@types/react": "npm:@types/react@^19.1.2",
        "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^4.4.1",
        "react": "npm:react@^19.1.0",
        "react-dom": "npm:react-dom@^19.1.0",
        "react-router-dom": "npm:react-router-dom@^7.5.1",
        "vite": "npm:vite@^6.3.2",
        "react-bootstrap": "npm:react-bootstrap@^1.6.6",
        "react-datepicker": "npm:react-datepicker@^3.8.0",
        "axios": "npm:axios@^0.21.2",
        "react-redux": "npm:react-redux@^7.2.4",
        "redux-persist": "npm:redux-persist@^6.0.0",
        "redux": "npm:redux@^4.0.5",
        "redux-thunk": "npm:redux-thunk@^2.3.0",
        "connected-react-router": "npm:connected-react-router@^6.9.1",
        "typesafe-actions": "npm:typesafe-actions@^5.1.0",
        "@fortawesome/fontawesome-free": "npm:@fortawesome/fontawesome-free@^6.0.0",
        "@fortawesome/fontawesome-svg-core": "npm:@fortawesome/fontawesome-svg-core@^1.3.0",
        "@fortawesome/free-solid-svg-icons": "npm:@fortawesome/free-solid-svg-icons@^6.0.0",
        "@fortawesome/react-fontawesome": "npm:@fortawesome/react-fontawesome@^0.1.17",
        "typestyle": "npm:typestyle@^2.1.0",
        "history": "npm:history@^4.7.2"
    },
    "compilerOptions": {
        "types": ["react", "react-dom", "@types/react"],
        "lib": ["dom", "dom.iterable", "deno.ns"],
        "jsx": "react-jsx",
        "jsxImportSource": "react"
    }
}
